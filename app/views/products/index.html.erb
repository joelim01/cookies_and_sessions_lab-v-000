<ul>
    <%= @items.each do |i| %>
      <li>
        <%=i%>
      </li>
      <%end%>
</ul>

<%= form_tag 'products/add', method: :post, class: "navbar-form navbar-left search-form" do %>
  <%= text_field_tag(
      :item,
      params[:item],
      placeholder: "Input Item",
      class: "form-control"
  ) %>
<% end %>
